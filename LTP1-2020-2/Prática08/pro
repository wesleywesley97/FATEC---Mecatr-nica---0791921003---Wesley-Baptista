palavra = "Kakaroto Goku"
#Acessa uma posição da palavra
letra = palavra[0]
#CUIDADO: não pode acessar uma posição que não existe
#letra = palavra[1000]
#Acessa um conjunto de letras de uma string
letras = palavra[1:5]
#Acessa um conjunto de letras de uma string
letras = palavra[5:]
#Acessa um conjunto de letras de uma string
letras = palavra[:8]
print(letras)
